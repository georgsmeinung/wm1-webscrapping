{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Web Scraper",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/1-web-scrapping.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONUNBUFFERED": "1"
            }
        },
        {
            "name": "Run HTML to DataFrame",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/2-html-a-dataframe.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONUNBUFFERED": "1"
            }
        },
        {
            "name": "Run Training and Evaluation",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/3-entrenar-y-evaluar.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONUNBUFFERED": "1"
            }
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONUNBUFFERED": "1"
            }
        }
    ]
}